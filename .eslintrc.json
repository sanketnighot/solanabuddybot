{
  "parser": "@typescript-eslint/parser",
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended",
    "prettier"
  ],
  "plugins": [
    "@typescript-eslint"
  ],
  "rules": {
    // Add any custom rules here
    "semi": "error",
    "no-console": [
      "error",
      {
        "allow": [
          "log"
        ]
      }
    ]
  },
  "env": {
    "node": true,
    "es6": true
  }
}